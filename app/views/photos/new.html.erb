<h1>画像投稿ページ</h1>
  <%= form_for(@photo, :url => { controller:'photos', action:'create'})do |f| %>
   <div class="field">
     <%= f.label :image %>
     <br>
     <%= f.file_field :image %>
     <br>
     <%= f.label :title %>
     <br>
     <%= f.text_field :title %>
     <br>
     <%= f.label :memo %>
     <br>
     <%= f.text_field :memo %>
   </div>
   <div class="action">
     <%= f.submit %>
   </div>
  <% end %>
  <a href="/">トップページへ</a>
  